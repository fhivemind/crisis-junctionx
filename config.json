{
	"data": {
		"filename": "lang.csv",
		"columns": [
			"lang"
		],
		"set_size": 5000,
		"sequence_length": 21,
		"train_test_split": 0.8,
		"normalise": false
	},
	"training": {
		"epochs": 14,
		"batch_size": 36
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 200,
				"input_timesteps": 20,
				"input_dim": 1,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 200,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 200,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
